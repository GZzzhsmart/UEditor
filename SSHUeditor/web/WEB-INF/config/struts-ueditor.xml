<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
    <package name="ueditorPackage" namespace="/ueditor" extends="basePackage">
        <action name="*" class="ueditorAction" method="{1}">
            <result name="delete" type="json">/</result>
            <result name="add" >/addUeditor.jsp</result>
            <result name="list">/ueditorList.jsp</result>
            <result name="update" type="json">/</result>
            <result name="find">/update.jsp</result>
        </action>
    </package>
</struts>